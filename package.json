{
  "name": "jefri",
  "title": "JEFRi: Entity Framework Runtime",
  "version": "1.2.4",
  "description": "A full-featured entity framework for javascript.",
  "homepage": "http://www.jefri.org",
  "bugs": "https://github.com/jefri/jefri-runtime/issues",
  "author": {
    "name": "David Souther",
    "email": "davidsouther@gmail.com",
    "url": "http://davidsouther.com"
  },
  "contributors": [
    {
      "name": "Jonathan Porta",
      "email": "jonathan@jonathanporta.com"
    }
  ],
  "files": [
    "lib/",
    "src/",
    "package.json"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ lib/ test/mocha/**/*.js && tsc -p src && webpack ./dist/index.js ./lib/jiffies.js && tsc -p test && webpack test/**/*.js test/tests.js",
    "pretest": "npm run build",
    "test": "mocha test/spec/{properties,smoke}.js && karma start karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jefri/jefri-runtime"
  },
  "keywords": [
    "ef",
    "entity",
    "entity framework"
  ],
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "browser": {
    "./src/index.js": "./src/browser.js"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "glob": "^6.0.1",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-mocha": "^0.2.1",
    "karma-spec-reporter": "0.0.22",
    "mocha": "^2.3.4",
    "typescript": "^1.6.2",
    "webpack": "^1.12.8"
  },
  "dependencies": {
    "jefri-jiffies": "~0.1.2"
  }
}
