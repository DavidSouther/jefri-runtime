{
  "name": "jefri",
  "title": "JEFRi: Entity Framework Runtime",
  "version": "1.2.5-pre2",
  "description": "A full-featured entity framework for javascript.",
  "homepage": "http://www.jefri.org",
  "bugs": "https://github.com/jefri/jefri-runtime/issues",
  "author": {
    "name": "David Souther",
    "email": "davidsouther@gmail.com",
